# Primary + fallbacks (comma-separated). First one should be your paid/Helius URL.
RPC_URLS=https://mainnet.helius-rpc.com/?api-key=<YOUR_KEY>,https://rpc.helius.xyz/?api-key=<YOUR_KEY>,https://api.mainnet-beta.solana.com
# GPA cache TTL in seconds (markets/custodies etc.)
GPA_CACHE_TTL=600
# Maximum RPC retries across the rotation
RPC_MAX_RETRIES=5

# Required for correct program slots and stable USD scaling
PERPS_CU_LIMIT=800000
PERPS_CU_PRICE=100000
PERPS_USD_SCALE=1000000
# Optional referral (defaults to wallet owner if unset)
JUP_PERPS_REFERRAL=
