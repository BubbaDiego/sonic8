{
  "$id": "schema/MonitorStatus.json",
  "type": "object",
  "required": [
    "timestamp"
  ],
  "properties": {
    "status": {
      "type": "string",
      "enum": [
        "ok",
        "degraded",
        "error"
      ]
    },
    "timestamp": {
      "type": "string",
      "format": "date-time"
    }
  },
  "additionalProperties": true,
  "examples": [
    {
      "timestamp": "2025-09-20T11:51:39Z",
      "status": "ok"
    }
  ]
}
