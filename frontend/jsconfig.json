{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "routes": ["src/routes", "src/routes/index.jsx", "src/routes/index.js"],
      "layout/*": ["src/layout/*"],
      "themes": ["src/themes", "src/themes/index.js"],
      "theme/*": ["src/theme/*"],
      "theme": ["src/theme"],
      "ui-component/*": ["src/ui-component/*"],
      "components/*": ["src/components/*"],
      "views/*": ["src/views/*"],
      "store/*": ["src/store/*"],
      "utils/*": ["src/utils/*"],
      "assets/*": ["src/assets/*"],
      "images/*": ["src/assets/images/*"],
      "menu-items": ["src/menu-items/index.js", "src/menu-items/index.jsx", "src/menu-items"],
      "menu-items/*": ["src/menu-items/*"],
      "contexts/*": ["src/contexts/*"],
      "hooks/*": ["src/hooks/*"],
      "config": ["src/config/index.js", "src/config/index.jsx", "src/config"],
      "config/*": ["src/config/*"],
      "api/*": ["src/api/*"]
    },
    "jsx": "react-jsx",
    "module": "esnext",
    "moduleResolution": "bundler",
    "target": "es2020",
    "checkJs": false
  },
  "include": ["src"]
}
